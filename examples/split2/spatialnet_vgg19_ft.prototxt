name: "SPATIAL_NET_VGG19"
layers {
  name: "data"
  type: VIDEO_DATA
  top: "data"
  top: "label"
  video_data_param {
    source: "data/ucf101/train2.txt"
    root_dir: "data/ucf101/ucf101_image/"
		shuffle: true
    batch_size: 64
		input_type: IMAGE
  }
  transform_param {
    crop_size: 224
		mean_file: "data/ucf101/ucf101_rgb_mean.binaryproto"
    mirror: true
  }
  include: { phase: TRAIN }
}
