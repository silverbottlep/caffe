    source: "data/ucf101/test2.txt"
    root_dir: "data/ucf101/ucf101_image/"
    batch_size: 50
		input_type: IMAGE
  }
  transform_param {
    crop_size: 224
		mean_file: "data/ucf101/ucf101_rgb_mean.binaryproto"
    mirror: false
  }
  include: { phase: TEST }
}
